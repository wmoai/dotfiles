{
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Hybrid",
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.fontFamily": "Monaco, Menlo, 'Courier New', monospace",
  "editor.formatOnSave": true,
  "eslint.alwaysShowStatus": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.editor.enablePreview": false,
  "workbench.editor.labelFormat": "medium",
  "explorer.compactFolders": false,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "scss", "typescriptreact"],
  "launch": {
    "version": "0.2.0",
    "compounds": [
      {
        "name": "Compound",
        "configurations": []
      }
    ]
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["g", "t"],
      "after": ["<Esc>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["g", "T"],
      "after": ["<Esc>"],
      "commands": ["workbench.action.previousEditor"]
    }
  ],
  "editor.parameterHints.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "json.schemas": [],
  "cSpell.userWords": ["Axios", "clsx", "linkify", "wizleap"],
  "vue.codeActions.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.openRepositoryInParentFolders": "never",
  "workbench.editor.tabSizing": "fixed",
  "workbench.editor.tabSizingFixedMaxWidth": 200
}
